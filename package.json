{
  "name": "mk-gql",
  "version": "0.6.0",
  "description": "Bindings for mobx-keystone and GraphQL",
  "authors": [
    "Austin Golding"
  ],
  "keywords": [
    "mobx-keystone",
    "graphql"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/kibeo/mk-gql#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kibeo/mk-gql.git"
  },
  "bugs": {
    "url": "https://github.com/kibeo/mk-gql/issues"
  },
  "license": "MIT",
  "main": "dist/mk-gql.js",
  "source": "src/mk-gql.ts",
  "umd:main": "dist/mk-gql.umd.js",
  "module": "dist/mk-gql.module.js",
  "jsnext:main": "dist/mk-gql.module.js",
  "react-native": "dist/mk-gql.module.js",
  "types": "./dist/mk-gql.d.ts",
  "bin": "./generator/mk-gql-scaffold.js",
  "files": [
    "dist",
    "generator"
  ],
  "scripts": {
    "test": "jest test && cd examples/2-scaffolding && yarn && yarn start",
    "watch": "jest test --watch",
    "build": "microbundle --compress --external mobx,mobx-keystone,graphql,graphql-request",
    "format": "prettier --write '**/*'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "camelcase": "^6.0.0",
    "fast-json-stable-stringify": "^2.1.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/preset-env": "^7.12.7",
    "@babel/preset-typescript": "^7.12.7",
    "@babel/register": "^7.12.1",
    "@types/jest": "^26.0.14",
    "@types/pluralize": "^0.0.29",
    "@types/throttle-debounce": "^2.1.0",
    "@types/ws": "7.4.0",
    "arg": "^4.1.3",
    "cosmiconfig": "^7.0.0",
    "escape-string-regexp": "^4.0.0",
    "graphql": "^14",
    "graphql-request": "^3.7.0",
    "graphql-tag": "^2.11.0",
    "husky": "^4.3.0",
    "jest": "^26.4.2",
    "microbundle": "^0.14.1",
    "mobx": "^6.3.6",
    "mobx-keystone": "^0.64.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "relative-deps": "^1.0.4",
    "tslib": "^2.2.0",
    "typescript": "^4.4.4"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": false
    },
    "graphql-request": {
      "optional": false
    },
    "mobx": {
      "optional": false
    },
    "mobx-keystone": {
      "optional": false
    }
  },
  "peerDependencies": {
    "graphql": "^14",
    "graphql-request": "^3",
    "mobx": "^4.0.0 || ^5.0.0 || ^6.0.0",
    "mobx-keystone": "^0.55.0"
  }
}
