{
  "name": "mk-gql",
  "version": "0.8.2",
  "description": "Bindings for mobx-keystone and GraphQL",
  "authors": [
    "Austin Golding"
  ],
  "workspaces": [
    "packages/**"
  ],
  "keywords": [
    "mobx-keystone",
    "graphql"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/GoldingAustin/mk-gql#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GoldingAustin/mk-gql.git"
  },
  "bugs": {
    "url": "https://github.com/GoldingAustin/mk-gql/issues"
  },
  "license": "MIT",
  "readme": "README.md",
  "main": "dist/mk-gql.js",
  "source": "core/src/mk-gql.ts",
  "umd:main": "dist/mk-gql.umd.js",
  "module": "dist/mk-gql.module.js",
  "jsnext:main": "dist/mk-gql.module.js",
  "react-native": "dist/mk-gql.module.js",
  "types": "./dist/mk-gql.d.ts",
  "bin": "./generator/mk-gql-scaffold.js",
  "files": [
    "dist",
    "generator"
  ],
  "scripts": {
    "test": "jest test",
    "build": "microbundle -i ./packages/core/src/mk-gql.ts --tsconfig ./tsconfig.build.json --compress --external mobx,mobx-keystone,graphql,graphql-request && yarn workspace generator run build",
    "format": "prettier --write './**/*.(ts|json)'"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "camelcase": "^6.3.0",
    "fast-json-stable-stringify": "^2.1.0",
    "pluralize": "^8.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.12.10",
    "@babel/plugin-proposal-class-properties": "7.12.13",
    "@babel/plugin-proposal-decorators": "7.12.13",
    "@babel/plugin-transform-modules-commonjs": "7.12.13",
    "@babel/preset-env": "7.12.17",
    "@babel/preset-typescript": "7.12.17",
    "@babel/register": "7.12.13",
    "@types/jest": "^27.5.1",
    "@types/pluralize": "^0.0.29",
    "@types/throttle-debounce": "^2.1.0",
    "@types/ws": "7.4.0",
    "arg": "^4.1.3",
    "cosmiconfig": "^7.0.1",
    "escape-string-regexp": "^4.0.0",
    "get-them-args": "^1.3.2",
    "graphql": "^16.5.0",
    "graphql-request": "^4.2.0",
    "graphql-tag": "^2.12.6",
    "husky": "^8.0.1",
    "is-installed": "^2.0.1",
    "jest": "^28.1.0",
    "latest-version": "^6.0.0",
    "microbundle": "^0.15.0",
    "mobx": "^6.6.0",
    "mobx-keystone": "^0.68.6",
    "prettier": "^2.6.2",
    "pretty-quick": "^3.1.3",
    "read-pkg-up": "^9.1.0",
    "relative-deps": "^1.0.7",
    "shell-exec": "^1.1.2",
    "ts-jest": "^28.0.3",
    "tslib": "^2.4.0",
    "typescript": "^4.7.2"
  },
  "peerDependenciesMeta": {
    "graphql": {
      "optional": false
    },
    "graphql-request": {
      "optional": false
    },
    "mobx": {
      "optional": false
    },
    "mobx-keystone": {
      "optional": false
    }
  },
  "peerDependencies": {
    "graphql": "^15 || ^16",
    "graphql-request": "^3",
    "mobx": "^4.0.0 || ^5.0.0 || ^6.0.0",
    "mobx-keystone": "^0"
  },
  "packageManager": "yarn@3.2.1"
}
