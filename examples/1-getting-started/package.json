    "@babel/preset-react": "^7.0.0",
    "@types/graphql": "^14.2.3",
    "@types/jest": "^24.0.16",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "apollo-server-express": "^2.8.1",
    "arg": "^4.1.1",
    "babel-loader": "^8.0.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "graphql": "^14.3.0",
    "graphql-playground-middleware-express": "^1.7.12",
    "graphql-tag": "^2.10.1",
