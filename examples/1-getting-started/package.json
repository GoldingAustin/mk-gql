    "start": "run-p start:server start:client",
    "type-check": "tsc --noEmit",
    "type-check:watch": "yarn type-check -- --watch"
  },
  "dependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
